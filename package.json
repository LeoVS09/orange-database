{
   "name": "orange-database",
   "version": "0.1.3",
   "description": "Database for Orange, based on PostgreSQL and GraphQL",
   "main": "index.js",
   "scripts": {
      "dev": "yarn build && yarn start:dev",
      "production": "yarn clean && yarn build:production",
      "build": "NODE_ENV=development babel ./src -d ./build --extensions \".ts\" --source-maps",
      "build:production": "NODE_ENV=production babel ./src -d ./build --extensions \".ts\"",
      "start": "NODE_ENV=production node ./build/index.js",
      "start:dev": "NODE_ENV=development yarn watch & yarn nodemon:start",
      "clean": "rm -rf build",
      "watch": "babel ./src --watch -d ./build --extensions \".ts\" --source-maps ",
      "nodemon:start": "nodemon ./build/index.js"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/LeoVS09/orange-database.git"
   },
   "author": "",
   "license": "GPL-3.0",
   "bugs": {
      "url": "https://github.com/LeoVS09/orange-database/issues"
   },
   "homepage": "https://github.com/LeoVS09/orange-database#readme",
   "dependencies": {
      "@graphile-contrib/pg-simplify-inflector": "^5.0.0-beta.1",
      "@koa/cors": "2.2.2",
      "graphile-utils": "^4.3.1",
      "http-proxy": "^1.17.0",
      "koa": "^2.7.0",
      "koa-bodyparser": "^4.2.1",
      "koa-bunyan-logger": "^2.1.0",
      "koa-compress": "^3.0.0",
      "koa-helmet": "^4.0.0",
      "koa-jwt": "^3.5.1",
      "koa-passport": "^4.1.1",
      "koa-route": "^3.2.0",
      "koa-session": "^5.10.0",
      "passport-github": "^1.1.0",
      "pg": "^7.10.0",
      "postgraphile": "^4.4.0-rc.1",
      "graphile-build-pg": "^4.3.1",
      "graphql": "^14.2.1",
      "jsonwebtoken": "^8.5.1"
   },
   "devDependencies": {
      "@babel/cli": "^7.1.5",
      "@babel/core": "^7.1.6",
      "@babel/plugin-proposal-class-properties": "^7.1.0",
      "@babel/plugin-proposal-export-default-from": "^7.0.0",
      "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
      "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
      "@babel/plugin-syntax-dynamic-import": "^7.0.0",
      "@babel/plugin-transform-classes": "^7.1.0",
      "@babel/plugin-transform-modules-commonjs": "^7.1.0",
      "@babel/plugin-transform-runtime": "^7.1.0",
      "@babel/preset-env": "^7.1.0",
      "@babel/preset-stage-2": "^7.0.0",
      "@babel/preset-typescript": "^7.1.0",
      "@babel/register": "^7.0.0",
      "@babel/runtime": "^7.1.2",
      "@types/koa": "^2.0.46",
      "@types/koa-bodyparser": "^5.0.1",
      "@types/koa-bunyan-logger": "^2.0.0",
      "@types/koa-compress": "^2.0.8",
      "@types/koa-helmet": "^3.1.2",
      "@types/koa-passport": "^4.0.2",
      "@types/koa-route": "^3.2.4",
      "@types/koa-session": "^5.7.4",
      "@types/koa__cors": "^2.2.3",
